{
  "name": "mdxlayer.js",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "release": "pnpm -r --filter './packages/*' publish --access public",
    "version-patch": "pnpm -r --filter './packages/*' version-patch",
    "git": "pnpm version-patch && read -p \"Enter commit message: \" msg; git add .; git commit -m \"$msg\"; git push",
    "clean": "pnpm -r --filter './packages/*' clean",
    "build": "pnpm -r --filter './packages/*' build",
    "lint": "pnpm -r --filter './packages/*' lint",
    "prebuild": "node readme.ts",
    "check-types": "pnpm -r check-types"
  },
  "devDependencies": {
    "@estarlincito/eslint": "^1.9.0",
    "@types/node": "^22.15.32",
    "esbuild": "^0.25.5",
    "eslint": "^9.29.0",
    "fast-glob": "^3.3.3",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.3"
  },
  "packageManager": "pnpm@10.4.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
