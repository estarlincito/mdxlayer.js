#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules/tyne/dist/cjs/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules/tyne/dist/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules/tyne/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules/tyne/dist/cjs/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules/tyne/dist/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules/tyne/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/tyne@1.0.1/node_modules:/Users/estarlincito/iDev/npm/mdxlayer.js/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tyne/dist/cjs/cli.cjs" "$@"
else
  exec node  "$basedir/../tyne/dist/cjs/cli.cjs" "$@"
fi
